{
  "name": "afrimomo",
  "packageManager": "pnpm@10.28.2",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "format": "pnpm -r format",
    "lint": "pnpm -r lint",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "release:beta": "changeset pre enter beta && pnpm version-packages && changeset pre exit",
    "release:major": "changeset major",
    "release:minor": "changeset minor",
    "release:patch": "changeset patch",
    "version:beta": "changeset version --snapshot beta",
    "publish:beta": "pnpm build && changeset publish --tag beta",
    "publish:release": "pnpm build && changeset publish --tag latest"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@changesets/cli": "^2.29.8",
    "conventional-changelog-cli": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "@conventional-changelog/git-client": ">=2.0.0",
      "axios": ">=1.7.9",
      "form-data": ">=4.0.1",
      "glob": ">=11.0.0",
      "qs": ">=6.13.1",
      "js-yaml": "3.14.1",
      "diff": ">=7.0.0",
      "tmp": ">=0.2.3",
      "on-headers": ">=1.0.3",
      "brace-expansion": ">=2.0.1",
      "@modelcontextprotocol/sdk": ">=1.0.7"
    }
  }
}
